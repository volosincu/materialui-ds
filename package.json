{
  "version": "0.0.4",
  "description": "",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "name": "materialui-ds",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
