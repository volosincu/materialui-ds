  "testMatch": [
    "**/__tests__/**/*.+(ts|tsx|js)",
    "**/?(*.)+(spec|test).+(ts|tsx|js)"
  ],
  "scripts": {
    "clean": "rimraf ./lib",
    "lint": "eslint ./components/* --ext .ts",
    "lint:fix": "npm run lint -- --fix",
