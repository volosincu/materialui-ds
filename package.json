  "dependencies": {
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.10.16",
    "@mui/styles": "^5.10.16",
    "react": "^18.2.0"
  },
  "files": [
    "lib/**/*",
    "package.json"
