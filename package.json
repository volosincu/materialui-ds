    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^3.2.0",
    "file-loader": "^6.2.0",
    "fs-extra": "^11.1.0",
    "html-webpack-plugin": "^4.5.2",
    "http-server": "^14.1.0",
    "jest": "^27.0.6",
    "jest-css-modules-transform": "^4.4.2",
    "jsdom": "^16.6.0",
    "jsdom-global": "^3.0.2",
